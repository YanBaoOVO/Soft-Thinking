    "import math\n",
    "\n",
    "# Define the probabilities\n",
    "p1 = 0.95\n",
    "p2 = 0.05\n",
    "\n",
    "# Calculate the entropy\n",
    "entropy = - (p1 * math.log2(p1) + p2 * math.log2(p2))\n",
    "\n",
    "# Print the result\n",
    "print(\"Entropy of the distribution (0.9, 0.1):\", entropy)\n"
   ]
